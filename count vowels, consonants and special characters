str=input("enter the string :")
v=c=d=s=0
for i in str:
    if(i=='a'or i=='e' or i=='i' or i=='o' or i=='u' and i=='A'or i=='E' or i=='I' or i=='O' or i=='U'):
        v+=1
    elif(i>='A' and i<='Z' or i>='a' and i<='z'):
        c+=1
    elif(i>='0' and i<='9'):
        d+=1
    else:
        s+=1
print(f"vowels={v}\nconsonants={c}\ndigits={d}\nspecial characters={s}")
