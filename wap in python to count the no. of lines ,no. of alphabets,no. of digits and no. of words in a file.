f=open("sample.txt",'r')
content=f.readlines()
print(len(content))
f=open("sample.txt",'r')
a=f.read()
alphabets=0
digits=0
for i in a:
      if (i>='A' and i<='Z' or i>='a' and i<='z'):
          alphabets+=1
print(alphabets) 
for i in a:
     if i>='0' and i<='9':
         digits+=1
print(digits)
a=a.split()
print(len(a))
